{
  "extends": "semistandard",
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "globals": {
    "alert": "readonly",
    "fetch": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}